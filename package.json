{
  "name": "astro-headstart",
  "description": "Astro components and utilities for managing the head of a HTML document",
  "version": "0.1.2",
  "private": false,
  "keywords": [
    "astro",
    "astro-component",
    "seo"
  ],
  "type": "module",
  "browser": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/san4d/astro-headstart.git"
  },
  "bugs": {
    "url": "https://github.com/san4d/astro-headstart/issues"
  },
  "license": "MIT",
  "main": "./src/Headstart.astro",
  "homepage": "https://github.com/san4d/astro-headstart#readme",
  "exports": {
    ".": {
      "types": "./src/Headstart.astro",
      "import": "./src/Headstart.astro"
    },
    "./meta": {
      "types": "./src/meta/index.ts",
      "import": "./src/meta/index.ts"
    },
    "./jsonld": {
      "types": "./src/jsonld/index.ts",
      "import": "./src/jsonld/index.ts"
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "schema-dts": "^1.1.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}